<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="508.0" prefWidth="791.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.baiyi.javaFxTool.controller.tool.FileAnaylizeController">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <center>
                <TextArea fx:id="watchLogTextArea" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets top="10.0" />
                    </BorderPane.margin>
                </TextArea>
            </center>
            <top>
                <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
                    <children>
                        <HBox alignment="CENTER" spacing="10.0">
                            <children>
                                <Label prefHeight="15.0" prefWidth="73.0" text="监 控 路 径：" />
                                <TextField fx:id="watchPathTextField" prefHeight="23.0" prefWidth="450.0" HBox.hgrow="ALWAYS" />
                                <Button fx:id="watchPathButton" mnemonicParsing="false" onAction="#watchPathAction" text="选择" />
                                <CheckBox fx:id="isShowNotificationCheckBox" mnemonicParsing="false" selected="true" text="显示桌面通知" />
                                <Button fx:id="watchButton" mnemonicParsing="false" onAction="#watchAction" text="监控" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="10.0" />
                        <HBox alignment="CENTER_LEFT" spacing="10.0">
                            <children>
                                <Label prefHeight="15.0" prefWidth="72.0" text="OURCODE:" />
                                <TextField fx:id="ourcode" prefHeight="23.0" prefWidth="167.0" />
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </top>
        </BorderPane>
    </children>
</AnchorPane>
